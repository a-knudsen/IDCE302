import math
def dist (x1, x2, y1, y2):
  dx = x2- x1
  dy = y2- y1
  print "dx is ", dx #this is a check
  print "dy is ", dy
  dsq = dx**2 + dy**2 #double astrix is to the power of..
  result = math.sqrt(dsq)
  return result
  

print dist (1, 7, 3, 8)
